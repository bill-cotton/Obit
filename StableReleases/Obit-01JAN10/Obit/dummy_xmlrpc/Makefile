ALL_CFLAGS = $(CFLAGS)

all: libxmlrpc.a

.c.o:
	$(CC) -c $(ALL_CPPFLAGS) $(ALL_CFLAGS) $<

libxmlrpc.a: xmlrpc.o
	$(AR) r $@ $<

clean:
	rm -f *.o
	rm -f *.a
